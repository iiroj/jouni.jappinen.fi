{
  "name": "@jouni.jappinen.fi/site",
  "version": "0.1.0",
  "description": "Static site made with Gatsby",
  "bugs": "https://github.com/iiroj/jouni.jappinen.fi/issues",
  "licence": "UNLICENCED",
  "author": "Jouni Jäppinen <jouni@jappinen.fi> (https://jouni.jappinen.fi)",
  "contributors": [
    "Iiro Jäppinen <iiro@jappinen.fi> (https://iiro.fi)"
  ],
  "repository": "github:iiroj/jouni.jappinen.fi",
  "scripts": {
    "develop": "gatsby develop",
    "start": "npm run develop",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "npm run lint",
    "build": "gatsby build",
    "predeploy": "npm run build",
    "deploy": "./bin/deploy.sh"
  },
  "dependencies": {
    "babel-eslint": "*",
    "babel-plugin-add-module-exports": "*",
    "babel-plugin-transform-class-properties": "*",
    "babel-preset-babili": "*",
    "babel-preset-es2015": "*",
    "babel-preset-react-hmre": "*",
    "babel-preset-react": "*",
    "babel-preset-stage-0": "*",
    "eslint-config-standard-react": "*",
    "eslint-config-standard": "*",
    "eslint-plugin-promise": "*",
    "eslint-plugin-react": "*",
    "eslint-plugin-standard": "*",
    "eslint": "*",
    "gatsby": "*",
    "react-dom": "*",
    "react-helmet": "*",
    "react": "*",
    "toml-js": "*"
  },
  "private": true,
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-class-properties",
      "add-module-exports"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "standard",
      "standard-react"
    ],
    "globals": {
      "__PREFIX_LINKS__": true
    }
  }
}

{
  "name": "jouni.jappinen.fi",
  "version": "8.23.1",
  "description": "Static site built with GatsbyJS",
  "author": "Jouni Jäppinen <jouni@jappinen.fi> (https://jouni.jappinen.fi)",
  "contributors": [
    "Iiro Jäppinen <iiro@jappinen.fi> (https://iiro.fi)"
  ],
  "license": "MIT",
  "repository": "github:iiroj/jouni.jappinen.fi",
  "bugs": "https://github.com/iiroj/jouni.jappinen.fi/issues",
  "private": true,
  "scripts": {
    "dev": "next",
    "lint": "eslint .",
    "build": "next build",
    "version": "standard-version --no-verify"
  },
  "lint-staged": {
    "*.js": "npm run lint"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "standard-version": {
    "scripts": {
      "prerelease": "npm run lint && npm run build"
    }
  },
  "dependencies": {
    "@emotion/core": "^10.0.21",
    "fontfaceobserver": "^2.1.0",
    "next": "9.1.1",
    "polished": "^3.4.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-helmet-async": "^1.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@emotion/babel-preset-css-prop": "^10.0.17",
    "babel-eslint": "^10.0.3",
    "babel-plugin-polished": "^1.1.0",
    "commitlint": "^8.2.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "husky": "^3.0.8",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "standard-version": "^7.0.0"
  },
  "keywords": [
    "react",
    "serverless",
    "static-site"
  ]
}

{
  "name": "jouni.jappinen.fi",
  "version": "8.22.6",
  "description": "Static site built with GatsbyJS",
  "author": "Jouni Jäppinen <jouni@jappinen.fi> (https://jouni.jappinen.fi)",
  "contributors": [
    "Iiro Jäppinen <iiro@jappinen.fi> (https://iiro.fi)"
  ],
  "license": "MIT",
  "repository": "github:iiroj/jouni.jappinen.fi",
  "bugs": "https://github.com/iiroj/jouni.jappinen.fi/issues",
  "private": true,
  "scripts": {
    "start": "gatsby develop",
    "lint": "eslint .",
    "prebuild": "rimraf public",
    "build": "gatsby build",
    "version": "standard-version --no-verify"
  },
  "lint-staged": {
    "*.js": "npm run lint"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "standard-version": {
    "scripts": {
      "prerelease": "npm run lint && npm run build"
    }
  },
  "dependencies": {
    "@emotion/core": "^10.0.16",
    "fontfaceobserver": "^2.1.0",
    "gatsby": "^2.14.0",
    "gatsby-image": "^2.2.14",
    "gatsby-link": "^2.2.9",
    "polished": "^3.4.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet-async": "^1.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@emotion/babel-preset-css-prop": "^10.0.14",
    "babel-eslint": "^10.0.3",
    "babel-plugin-polished": "^1.1.0",
    "commitlint": "^8.1.0",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "gatsby-plugin-netlify": "^2.1.10",
    "gatsby-plugin-sharp": "^2.2.18",
    "gatsby-source-filesystem": "^2.1.18",
    "gatsby-transformer-sharp": "^2.2.12",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "standard-version": "^7.0.0",
    "typescript": "^3.5.3"
  },
  "keywords": [
    "gatsby",
    "gatsbyJS",
    "netlify",
    "react",
    "serverless",
    "static-site"
  ]
}

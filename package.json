{
  "name": "jouni.jappinen.fi",
  "version": "1.0.0",
  "description": "Static site made with Gatsby",
  "bugs": "https://github.com/iiroj/jouni.jappinen.fi/issues",
  "licence": "UNLICENCED",
  "author": "Jouni Jäppinen <jouni@jappinen.fi> (https://jouni.jappinen.fi)",
  "contributors": [
    "Iiro Jäppinen <iiro@jappinen.fi> (https://iiro.fi)"
  ],
  "repository": "github:iiroj/jouni.jappinen.fi",
  "scripts": {
    "develop": "gatsby develop",
    "start": "npm run develop",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "npm run lint",
    "build": "gatsby build",
    "deploy": "s3cmd sync ./public/ s3://jouni.jappinen.fi --delete-removed --guess-mime-type --no-mime-magic --no-preserve --server-side-encryption --cf-invalidate",
    "publish": "npm run build && npm run deploy"
  },
  "dependencies": {
    "react": "15.4.2",
    "react-dom": "15.4.2"
  },
  "devDependencies": {
    "babel-eslint": "7.1.1",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-class-properties": "6.23.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-0": "6.22.0",
    "eslint": "^3.17.0",
    "eslint-config-react": "^1.1.7",
    "eslint-config-standard": "^7.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "6.10.0",
    "eslint-plugin-standard": "^2.1.1",
    "gatsby": "^0.12.38",
    "react-helmet": "4.0.0",
    "reset.css": "2.0.2"
  },
  "private": true
}

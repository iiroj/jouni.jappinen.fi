{
  "name": "jouni.jappinen.fi",
  "version": "8.41.0",
  "description": "Static site built with Next.js",
  "author": "Jouni Jäppinen <jouni@jappinen.fi> (https://jouni.jappinen.fi)",
  "contributors": [
    "Iiro Jäppinen <iiro@jappinen.fi> (https://iiro.fi)"
  ],
  "license": "MIT",
  "repository": "github:iiroj/jouni.jappinen.fi",
  "bugs": "https://github.com/iiroj/jouni.jappinen.fi/issues",
  "private": true,
  "workspaces": [
    "site"
  ],
  "scripts": {
    "start": "npm run --workspace=site start",
    "lint": "eslint --ext js,jsx .",
    "build": "npm run --workspaces build",
    "serverless:state:download": "aws s3 sync s3://jouni.jappinen.fi-serverless-state .serverless --delete",
    "serverless:deploy": "serverless --debug",
    "serverless:state:upload": "aws s3 sync .serverless s3://jouni.jappinen.fi-serverless-state --delete",
    "version": "standard-version --no-verify"
  },
  "dependencies": {
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@sls-next/serverless-component": "3.6.0",
    "commitlint": "15.0.0",
    "eslint": "8.4.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "husky": "7.0.4",
    "lint-staged": "12.1.2",
    "prettier": "2.5.1",
    "serverless": "2.68.0",
    "standard-version": "9.3.2"
  }
}
